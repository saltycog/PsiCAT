# PsiCAT Discord Bot Configuration
# Copy this file to .env and fill in your values

# Discord Bot Configuration
# Generate a bot token at: https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_bot_token_here

# Discord Guild (Server) ID where commands will be registered
# Right-click your server in Discord and select "Copy Server ID" (enable Developer Mode first)
DISCORD_GUILD_ID=your_guild_id_here

# Application Configuration
# URL where avatars will be accessible
AVATAR_BASE_URL=http://localhost:5247

# ASP.NET Environment
ASPNETCORE_ENVIRONMENT=Production
